{
  "name": "progressive-web-apps-1920",
  "version": "1.0.0",
  "description": "progressive Dota2 web app",
  "main": "docs/server.js",
  "directories": {
    "doc": "docs",
    "example": "examples"
  },
  "scripts": {
    "prestart": "npm run fetch & npm run build:css",
    "prebuild:css": "rimraf /docs/static/css",
    "build:css": "node scripts/build-css.js",
    "watch:css": "chokidar /docs/src/css/*.css -C \"npm run build:css\"",
    "test": "npm run dev",
    "start": "node docs/server.js",
    "dev": "nodemon docs/server.js",
    "fetch": "node scripts/write-JSON.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Mokerstier/progressive-web-apps-1920.git"
  },
  "keywords": [
    "Web",
    "app",
    "DOTA",
    "DOTA2",
    "data"
  ],
  "author": "Wouter van der Heijde",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Mokerstier/progressive-web-apps-1920/issues"
  },
  "homepage": "https://github.com/Mokerstier/progressive-web-apps-1920#readme",
  "dependencies": {
    "bcrypt": "^4.0.1",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.5",
    "ejs": "^3.0.1",
    "express": "^4.17.1",
    "express-session": "^1.17.0",
    "fs-extra": "^8.1.0",
    "mongodb": "^3.5.5",
    "mongoose": "^5.9.5",
    "mongoose-unique-validator": "^2.0.3",
    "node-fetch": "^2.6.0",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0"
  },
  "devDependencies": {
    "chokidar-cli": "^2.1.0",
    "dotenv": "^8.2.0",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^7.0.1",
    "gulp-clean-css": "^4.2.0",
    "gulp-concat": "^2.6.1",
    "nodemon": "^2.0.2",
    "rimraf": "^3.0.2"
  }
}
