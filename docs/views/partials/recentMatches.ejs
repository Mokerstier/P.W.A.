<main>
    <% for(let i=0; i < matches.length; i++) {%>
        <article>
            <ul>
                <li><%= new Date(matches[i].start_time * 1000).toISOString().slice(0, 19).replace('T', ' ') %></li>
                <li>Match duration: <%= Math.floor(matches[i].duration / 60) %>min.</li>
                <li>Played with: <%= heroName[i] %></li>
                <li class="hero-kills">Kills: <%= matches[i].kills%></li>
                <li class="hero-deaths">Deaths: <%= matches[i].deaths%></li>
                <li class="hero-assists">Assists: <%= matches[i].assists%></li>
            </ul>
        </article>
    <%}%>
</main>